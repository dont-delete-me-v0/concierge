{
  "url": "https://concert.ua/uk/catalog/kyiv/concerts/date-from=2025-10-07&date-till=2025-11-17",
  "waitFor": [".event-card", ".item-card"],
  "pagination": {
    "type": "infinite-scroll",
    "scrollDelay": 3000
  },
  "selectors": [
    {
      "name": "title",
      "selector": ".item-card-title",
      "type": "text",
      "multiple": true,
      "transform": "trim"
    },
    {
      "name": "price",
      "selector": ".item-card .fs-16",
      "type": "text",
      "multiple": true,
      "transform": "trim"
    },
    {
      "name": "link",
      "selector": ".item-card",
      "type": "attribute",
      "attribute": "href",
      "multiple": true
    },
    {
      "name": "eventId",
      "selector": ".item-card",
      "type": "attribute",
      "attribute": "data-id",
      "multiple": true
    },
    {
      "name": "dateTime",
      "selector": ".item-card .fw-900.fs-14",
      "type": "text",
      "multiple": true,
      "transform": "trim"
    },
    {
      "name": "venue",
      "selector": ".item-card .color-secondary.fs-14",
      "type": "text",
      "multiple": true,
      "transform": "trim"
    }
  ],
  "incremental": {
    "enabled": true,
    "uniqueKey": ["title", "venue", "dateTime"]
  }
}
